{
  "name": "server",
  "version": "1.0.0",
  "description": "express server",
  "main": "index.js",
  "engines": {
    "node": "9.5.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
